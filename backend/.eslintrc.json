{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "import/extensions": 0,
        "import/no-unresolved": 0,
        "import/no-extraneous-dependencies": 0,
        "no-useless-constructor": 0,
        "no-unused-vars": 0,
        "no-empty-function": 0,
        "no-underscore-dangle": 0
    },
    "ignorePatterns": ["**/*.test.ts"] 
}
